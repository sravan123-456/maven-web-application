pipeline {
    agent any
    tools{
        maven 'maven3.8.3'
    }
    options{
        timestamps()
    }
    stages{
        stage('git checkout'){
            steps{
                git credentialsId: 'github-auth', url: 'https://github.com/sravan123-456/maven-web-application.git'
            }
            
        }
        stage('Build'){
            steps{
                sh "mvn clean package"
            }
        }
    }
}